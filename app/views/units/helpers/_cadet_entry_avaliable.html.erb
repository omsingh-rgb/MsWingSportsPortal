<% cadets.each do |cadet| %>

  <nav class="level">
    <div class="level-left">

      <%= render 'units/helpers/cadet_info_as_level_items', c:cadet %>

      <% disabled = 'title="Disabled button" disabled' %>

      <div class="level-item" >
        <a class='button is-success is-outlined'  href="" <%= disabled %>>
          <span>Add</span>
          <span class='icon is-small'>
                  &check;
          </span>
        </a>
      </div>
    </div>
  </nav>

<% end %>